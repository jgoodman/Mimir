% layout 'default';
% title 'Mimir - Status';
<h1><%= uc($name) %></h2>
% foreach my $branch (@$branches) {
    <h2><a href="/branch/<%= $branch->{'branch_id'} %>"><%= $branch->{'title'} %></a></h2>
    <ul>
    % foreach my $node (@{$branch->{'nodes'}}) {
        <li><a href="/node/<%= $node->{'node_id'} %>"><%= $node->{'title'} %></a></li>
    % }
    </ul>
% }

